{
  "name": "openclaw-setup",
  "version": "1.0.2",
  "type": "module",
  "description": "One-click OpenClaw installer with EvoLink integration",
  "bin": {
    "openclaw-setup": "./bin/setup.js"
  },
  "scripts": {
    "start": "node bin/setup.js",
    "build": "node scripts/build.js",
    "package": "pkg dist/setup.cjs --targets node22-win-x64,node22-macos-x64,node22-macos-arm64,node22-linux-x64 --output dist/openclaw-setup --compress GZip"
  },
  "dependencies": {
    "express": "^5.1.0"
  },
  "devDependencies": {
    "esbuild": "^0.25.0",
    "@yao-pkg/pkg": "^6.5.0"
  },
  "engines": {
    "node": ">=22.12.0"
  },
  "license": "MIT"
}
